                    <Spinner
                        android:id="@+id/T4ZipLine"
                        android:layout_width="fill_parent"
                        android:layout_height="32dp"
                        android:layout_weight="1"
                        android:hint="0-3"
                        android:inputType="phone"
                        android:popupBackground="@color/alliance_blue" />

                    <!--THIS ONE HAS TO BE FILLED WITH FLOOR, MID, HIGH, AND PULL-UP-->
                    <Spinner
                        android:id="@+id/T4ParkingEndgame"
                        android:layout_width="fill_parent"
                        android:layout_height="32dp"
                        android:layout_weight="1"
                        android:popupBackground="@color/alliance_blue" />

                    <Spinner
                        android:id="@+id/T4AllClear"
                        android:layout_width="fill_parent"
                        android:layout_height="32dp"
                        android:layout_weight="1"
                        android:hint="Y/N"
                        android:popupBackground="@color/alliance_blue" />
                </TableRow>

            </TableLayout>
    </ScrollView>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1">

        <ImageButton
            android:id="@+id/TeleOpBack"
            android:layout_width="112dp"
            android:layout_height="28dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:adjustViewBounds="true"
            android:background="@color/light_blue"
            android:scaleType="fitXY"
            android:src="@drawable/back" />

        <ImageButton
            android:id="@+id/TeleOpNotes"
            android:layout_width="112dp"
            android:layout_height="28dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:adjustViewBounds="true"
            android:background="@color/light_blue"
            android:scaleType="fitXY"
            android:src="@drawable/notes" />

        <ImageButton
            android:id="@+id/TeleOpNext"
            android:layout_width="112dp"
            android:layout_height="28dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:adjustViewBounds="true"
            android:background="@color/light_blue"
            android:scaleType="fitXY"
            android:src="@drawable/done" />

    </RelativeLayout>
</LinearLayout>